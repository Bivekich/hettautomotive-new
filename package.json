{
  "name": "hett-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "init:db": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "import:products": "node src/imports/directImport.js"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.31.0",
    "@payloadcms/next": "^3.31.0",
    "@payloadcms/payload-cloud": "^3.31.0",
    "@payloadcms/richtext-lexical": "^3.31.0",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.7",
    "formidable": "^3.5.2",
    "graphql": "^16.8.1",
    "next": "15.2.2",
    "payload": "^3.31.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.2",
    "express": "^4.21.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
